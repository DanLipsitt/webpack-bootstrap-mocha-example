{
  "name": "webpack-bootstrap-mocha-example",
  "private": true,
  "version": "1.0.0",
  "description": "Demo to explore how to get react-bootstrap working with webpack and mocha.",
  "main": "index.js",
  "scripts": {
    "test": "mocha --compilers js:babel/register /tmp/bootstrapTestBundle.js",
    "pretest": "webpack --config webpack.config.test.js",
    "prebuild": "cp client/index.html build",
    "build": "webpack",
    "predev": "npm run prebuild",
    "dev": "webpack-dev-server --devtool cheap-module-source-map --progress --colors --hot --content-base build"
  },
  "author": "Dan Lipsitt",
  "license": "UNLICENSED",
  "dependencies": {
    "jquery": "^2.1.4",
    "react": "^0.14.3",
    "react-bootstrap": "^0.28.1",
    "react-dom": "^0.14.3"
  },
  "devDependencies": {
    "babel": "^5.8.34",
    "babel-loader": "^5.4.0",
    "bootstrap": "^3.3.6",
    "bootstrap-webpack": "0.0.5",
    "chai": "^3.4.1",
    "css-loader": "^0.23.0",
    "exports-loader": "^0.6.2",
    "extract-text-webpack-plugin": "^0.9.1",
    "file-loader": "^0.8.5",
    "imports-loader": "^0.6.5",
    "less": "^2.5.3",
    "less-loader": "^2.2.1",
    "mocha": "^2.3.4",
    "null-loader": "^0.1.1",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DanLipsitt/webpack-bootstrap-mocha-example.git"
  },
  "bugs": {
    "url": "https://github.com/DanLipsitt/webpack-bootstrap-mocha-example/issues"
  },
  "homepage": "https://github.com/DanLipsitt/webpack-bootstrap-mocha-example#readme"
}
